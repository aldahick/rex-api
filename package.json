{
  "name": "@aldahick/rex-api",
  "version": "0.3.0",
  "description": "Personal API server",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "start:dev": "tsc-watch --onSuccess \"node dist/index.js\" | bunyan",
    "docker:up": "cross-env COMPOSE_PROJECT_NAME=rex docker-compose up",
    "build": "tsc",
    "lint": "eslint --ext .ts .",
    "generate": "yarn generate:graphql",
    "generate:graphql": "graphql-codegen --config codegen.graphql.yml",
    "createToken": "node scripts/createToken.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aldahick/rex-api.git"
  },
  "author": "Alex Hicks <alex@alexhicks.net> (https://alexhicks.net)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aldahick/rex-api/issues"
  },
  "homepage": "https://github.com/aldahick/rex-api#readme",
  "dependencies": {
    "@athenajs/core": "^0.7.2",
    "@typegoose/typegoose": "^7.4.5",
    "accesscontrol": "^2.2.1",
    "aws-sdk": "^2.817.0",
    "axios": "^0.21.1",
    "bcrypt": "^5.0.0",
    "cheerio": "^1.0.0-rc.5",
    "discord.js": "^12.5.1",
    "fs-extra": "^9.0.1",
    "gamedig": "^2.0.23",
    "googleapis": "^66.0.0",
    "graphql": "^15.4.0",
    "joi": "^17.3.0",
    "lodash": "^4.17.20",
    "mime": "^2.4.7",
    "pluralize": "^8.0.0",
    "randomstring": "^1.1.5",
    "reflect-metadata": "^0.1.13",
    "source-map-support": "^0.5.19",
    "tsyringe": "^4.4.0"
  },
  "devDependencies": {
    "@aldahick/eslint-rules": "^0.11.1",
    "@graphql-codegen/cli": "^1.19.4",
    "@graphql-codegen/typescript": "^1.19.0",
    "@graphql-codegen/typescript-operations": "^1.17.12",
    "@types/bcrypt": "^3.0.0",
    "@types/cheerio": "^0.22.23",
    "@types/cors": "^2.8.9",
    "@types/fs-extra": "^9.0.5",
    "@types/gamedig": "^2.0.1",
    "@types/lodash": "^4.14.165",
    "@types/mime": "^2.0.3",
    "@types/mongoose": "^5.10.3",
    "@types/node": "^14.14.14",
    "@types/pluralize": "^0.0.29",
    "@types/randomstring": "^1.1.6",
    "@types/recursive-readdir": "^2.2.0",
    "cross-env": "^7.0.2",
    "tsc-watch": "^4.2.9",
    "typescript": "^4.1.3"
  }
}
