{
  "name": "@aldahick/rex-api",
  "version": "0.1.0",
  "description": "Personal API server",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "start:dev": "tsc-watch --onSuccess \"node dist/index.js\" | bunyan",
    "docker:up": "cross-env COMPOSE_PROJECT_NAME=rex docker-compose up",
    "build": "tsc",
    "lint": "eslint --ext .ts .",
    "generate": "npm run generate:graphql",
    "generate:graphql": "graphql-codegen --config codegen.graphql.yml",
    "createToken": "node scripts/createToken.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aldahick/rex-api.git"
  },
  "author": "Alex Hicks <alex@alexhicks.net> (https://alexhicks.net)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aldahick/rex-api/issues"
  },
  "homepage": "https://github.com/aldahick/rex-api#readme",
  "dependencies": {
    "@athenajs/core": "^0.5.0",
    "@hapi/joi": "^17.1.1",
    "@microsoft/signalr": "^3.1.5",
    "@typegoose/typegoose": "^7.3.0",
    "accesscontrol": "^2.2.1",
    "axios": "^0.19.2",
    "bcrypt": "^5.0.0",
    "cheerio": "^1.0.0-rc.3",
    "discord.js": "^12.2.0",
    "dockerode": "^3.2.0",
    "fs-extra": "^9.0.1",
    "gamedig": "^2.0.21",
    "googleapis": "^52.1.0",
    "graphql": "^14.7.0",
    "lodash": "^4.17.19",
    "mime": "^2.4.6",
    "pluralize": "^8.0.0",
    "randomstring": "^1.1.5",
    "reflect-metadata": "^0.1.13",
    "source-map-support": "^0.5.19",
    "tsyringe": "^4.3.0"
  },
  "devDependencies": {
    "@aldahick/eslint-rules": "^0.8.1",
    "@graphql-codegen/cli": "^1.17.0",
    "@graphql-codegen/typescript": "^1.17.0",
    "@graphql-codegen/typescript-operations": "^1.17.0",
    "@types/bcrypt": "^3.0.0",
    "@types/cheerio": "^0.22.21",
    "@types/cors": "^2.8.6",
    "@types/dockerode": "^2.5.34",
    "@types/fs-extra": "^9.0.1",
    "@types/gamedig": "^1.0.1",
    "@types/lodash": "^4.14.157",
    "@types/mime": "^2.0.2",
    "@types/mongoose": "^5.7.31",
    "@types/node": "^14.0.22",
    "@types/pluralize": "0.0.29",
    "@types/randomstring": "^1.1.6",
    "@types/recursive-readdir": "^2.2.0",
    "cross-env": "^7.0.2",
    "tsc-watch": "^4.2.9",
    "typescript": "^3.9.6"
  }
}
